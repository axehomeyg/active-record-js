{
  "name": "active-record-js",
  "version": "2.0.27",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "integration-test": "./integration_tests/run.sh",
    "integration-test-run": "NODE_PATH=./src jest tests/integration_tests",
    "prepublishOnly": "npm run build",
    "test": "npm run unit-test && npm run integration-test",
    "unit-test": "NODE_PATH=./src jest tests/unit_tests --coverage",
    "watch": "tsc --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "author": "Matthew Wolfe",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/sqlstring": "2.2.1",
    "jest": "24.8.0",
    "sqlstring": "2.3.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@types/md5": "2.1.33",
    "@types/mysql": "2.15.6",
    "@types/node": "12.0.12",
    "md5": "2.2.1",
    "mysql": "2.17.1"
  }
}
